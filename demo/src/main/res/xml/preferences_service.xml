<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto">

    <EditTextPreference
        app:defaultValue="2022CODECTST"
        app:key="key_service_id"
        app:title="Service ID"
        app:useSimpleSummaryProvider="true" />

    <SwitchPreference
        app:key="key_use_external"
        app:title="Use external provisioning" />

    <EditTextPreference
        app:defaultValue="2022CODECTST0000"
        app:key="key_service_key"
        app:title="Service Key"
        app:useSimpleSummaryProvider="true" />

    <EditTextPreference
        app:defaultValue="2022CODECTST0000"
        app:key="key_secret"
        app:title="Secret"
        app:useSimpleSummaryProvider="true" />

    <EditTextPreference
        app:key="key_token"
        app:title="Token"
        app:useSimpleSummaryProvider="true" />

    <EditTextPreference
        app:defaultValue="https://icl2-provisioning-vc.k9ertc.io/api/v1/rpc"
        app:key="key_provisioning_url"
        app:title="Provisioning Server"
        app:useSimpleSummaryProvider="true" />

    <EditTextPreference
        app:key="key_api_url"
        app:title="API Server"
        app:useSimpleSummaryProvider="true" />

    <EditTextPreference
        app:key="key_turn_server"
        app:title="TURN Server"
        app:useSimpleSummaryProvider="true" />

    <EditTextPreference
        app:key="key_turn_username"
        app:title="TURN Username"
        app:useSimpleSummaryProvider="true" />

    <EditTextPreference
        app:key="key_turn_credential"
        app:title="TURN Credential"
        app:useSimpleSummaryProvider="true" />

</PreferenceScreen>