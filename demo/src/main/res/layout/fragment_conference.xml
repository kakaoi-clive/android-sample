<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.conference.ConferenceFragment">

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:columnCount="2"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <include
            android:id="@+id/cell_0"
            style="@style/CellRemoteVideo"
            layout="@layout/cell_remote_video" />

        <include
            android:id="@+id/cell_1"
            style="@style/CellRemoteVideo"
            layout="@layout/cell_remote_video" />

        <include
            android:id="@+id/cell_2"
            style="@style/CellRemoteVideo"
            layout="@layout/cell_remote_video" />

        <include
            android:id="@+id/cell_3"
            style="@style/CellRemoteVideo"
            layout="@layout/cell_remote_video" />

        <include
            android:id="@+id/cell_4"
            style="@style/CellRemoteVideo"
            layout="@layout/cell_remote_video" />

        <include
            android:id="@+id/cell_5"
            style="@style/CellRemoteVideo"
            layout="@layout/cell_remote_video" />

        <include
            android:id="@+id/cell_6"
            style="@style/CellRemoteVideo"
            layout="@layout/cell_remote_video" />

        <include
            android:id="@+id/cell_7"
            style="@style/CellRemoteVideo"
            layout="@layout/cell_remote_video" />

    </GridLayout>

</androidx.constraintlayout.widget.ConstraintLayout>